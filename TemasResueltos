using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;


namespace Numerico
{
    static void main(string[] args)
    {
        /// Cadena numerica

        //Variables

        string dato = "";
        int valor = 0;
        double valorf = 0.0;

        //Numero

        Console.WriteLine("Ingresa el numero");
        dato = Console.ReadLine();
        valor = Convert.ToInt32(dato);

        //Resultados

        Console.WriteLine("Ingresa el numero flotante");
        dato = Console.ReadLine();
        valorf = Convert.ToDouble(dato);

        Console.WriteLine("El numero flotante es {0}" , valorf)

    }
}

