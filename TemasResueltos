using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;


namespace Cadenas
{
    static void main(string[] args)
    {
        /// Obtencion de datos del usuario

        //Variables
        string nombre = "";
        string apellido = "";

        //Obtencion de variable

        Console.WriteLine("Dame tu nombre");
        nombre = Console.ReadLine();

        Console.WriteLine("Dame tu apellido");
        apellido = Console.ReadLine();
        
        //Saludo

        Console.WriteLine("Hola {0} {1}" , nombre, apellido);

    }
}
